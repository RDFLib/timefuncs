PREFIX time: <http://www.w3.org/2006/time#>
BASE <http://w3id.org/time-function/testdata/before/>

<ref1a> time:hasTime <refTE1a> .
<x1a> time:hasTime <xTE1a> .
<xTE1a> time:before <refTE1a> .

<ref1b> time:hasTime <refTE1b> .
<x1b> time:hasTime [
    time:before <refTE1b>
] .

<ref2a> time:hasTime <refTE2a> .
<refTE2a> time:after <xTE2a> .
<x2a> time:hasTime <xTE2a> .

<ref2b> time:hasTime [
    time:after <xTE2b>
] .
<x2b> time:hasTime <xTE2b> .


<ref3> time:hasTime [
    time:inXSDDateTimeStamp "2021-07-16T00:00:00Z" ] .

<x3> time:hasTime [
    time:inXSDDateTimeStamp "2021-07-15T23:59:59Z"
] .

<ref4> time:hasTime [
    time:hasBeginning [
        time:inXSDDateTimeStamp "2021-07-16T00:00:00Z"
    ] ;
] .

<x4> time:hasTime [
    time:hasEnd [
        time:inXSDDateTimeStamp "2021-07-15T23:59:59Z"
    ] ;
] .